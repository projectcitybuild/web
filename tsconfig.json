{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "lib": ["dom", "es2017"],
        "strict": true
    },
    "exclude": [
        "node_modules",
        "vendor",
    ],
}
