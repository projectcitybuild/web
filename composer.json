{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "altek/eventually": "^2.0",
        "awssat/discord-notification-channel": "^1.4",
        "bacon/bacon-qr-code": "^2.0",
        "doctrine/dbal": "^2.6",
        "guzzlehttp/guzzle": "^7.0.1",
        "jfcherng/php-diff": "^6.11",
        "laravel/cashier": "^13.8.0",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10.4",
        "laravel/scout": "^9.4.5",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.0",
        "league/commonmark": "^2.3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.12.0",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-sitemap": "^6.1.0",
        "stripe/stripe-php": "^7.92.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^11.0",
        "xpaw/php-minecraft-query": "^4.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^6.4.4",
        "fakerphp/faker": "^1.19",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "laravel/pint": "^0.2.0",
        "laravel/sail": "^1.13.",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1.3",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Entities\\": "entities/",
            "Domain\\": "domain/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/",
            "Library\\": "library/",
            "Repositories\\": "repositories/",
            "Helpers\\": "helpers/",
            "Shared\\": "shared/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=telescope-assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan telescope:publish --ansi"
        ]
    }
}
