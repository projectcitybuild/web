{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "abraham/twitteroauth": "^0.9.2",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "0.9.0",
        "squizlabs/php_codesniffer": "^3.3",
        "xpaw/php-minecraft-query": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.0.*",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "infrastructure/database/seeds",
            "infrastructure/database/factories"
        ],
        "psr-4": {
            "Application\\": "application/",
            "Application\\Console\\": "application/console/",
            "Application\\Middleware\\": "application/middleware/",
            "Application\\Providers\\": "application/providers/",
            "Domains\\": "domains/",
            "Domains\\Library\\": "domains/library/",
            "Domains\\Services\\": "domains/services/",
            "Entities\\": "entities/",
            "Support\\": "support/",
            "Infrastructure\\": "infrastructure/",
            "Infrastructure\\Helpers\\": "infrastructure/helpers/",
            "Interfaces\\": "interfaces/",
            "Interfaces\\Api\\": "interfaces/api/",
            "Interfaces\\Api\\Controllers\\": "interfaces/api/controllers/",
            "Interfaces\\Api\\Requests\\": "interfaces/api/requests/",
            "Interfaces\\Console\\": "interfaces/console/",
            "Interfaces\\Web\\": "interfaces/web/",
            "Interfaces\\Web\\Controllers\\": "interfaces/web/controllers/",
            "Interfaces\\Web\\Composers\\": "interfaces/web/composers/",
            "Interfaces\\Web\\Requests\\": "interfaces/web/requests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "vendor-dir": "support/packages/vendor",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
